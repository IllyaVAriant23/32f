#include <stdio.h>

int canFormTriangle(float a, float b, float c) {
    if (a <= 0 || b <= 0 || c <= 0)
        return 0; 

    if (a + b > c && a + c > b && b + c > a)
        return 1; 

    return 0; 

int main() {
    float a, b, c;

    printf("Введіть довжину першого відрізка: ");
    scanf("%f", &a);

    printf("Введіть довжину другого відрізка: ");
    scanf("%f", &b);

    printf("Введіть довжину третього відрізка: ");
    scanf("%f", &c);

    if (canFormTriangle(a, b, c)) {
        printf("Можна утворити трикутник.\n");
    } else {
        printf("Не можна утворити трикутник.\n");
    }

    return 0;
}
